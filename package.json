{
  "name": "retirement-tracking-app",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayovev/retirement-tracking-app"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.2.0",
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.2"
  },
  "scripts": {
    "client": "cd client && npm start",
    "database": "mongod --logpath /data/db/logs/mongodb.log",
    "server": "cd server && npm start",
    "setup": "npm install && npm run setup-client && npm run setup-server",
    "setup-client": "cd client && npm install",
    "setup-server": "cd server && npm install",
    "start": "concurrently \"npm run database\" \"npm run server\" \"npm run client\" --kill-others-on-fail",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
