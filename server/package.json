{
  "name": "retirement-tracking-app",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayovev/retirement-tracking-app"
  },
  "private": true,
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.2"
  },
  "scripts": {
    "client": "cd ../client && npm start",
    "server": "nodemon src/server.js",
    "setup": "npm install && cd ../client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\" --kill-others-on-fail"
  }
}
